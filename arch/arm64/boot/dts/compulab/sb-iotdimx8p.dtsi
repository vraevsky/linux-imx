&i2c5 {
	status = "okay";

	di8o8_0: pca9555-di8o8-0@20 {
		compatible = "nxp,pca9555";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_di8o8_0>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupt-parent = <&gpio5>;
		interrupts = <22 IRQ_TYPE_LEVEL_LOW>;
		reg = <0x20>;
		gpio-line-names =
			"IN0",  "IN1",  "IN2",  "IN3",
			"IN4",  "IN5",  "IN6",  "IN7",
			"OUT0", "OUT1", "OUT2", "OUT3",
			"OUT4", "OUT5", "OUT6", "OUT7";
