		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupt-parent = <&gpio5>;
		interrupts = <23 IRQ_TYPE_LEVEL_LOW>;
		reg = <0x21>;
		gpio-line-names =
			"IN0",  "IN1",  "IN2",  "IN3",
			"IN4",  "IN5",  "IN6",  "IN7",
			"OUT0", "OUT1", "OUT2", "OUT3",
			"OUT4", "OUT5", "OUT6", "OUT7";
	};

	di8o8_2: di8o8-2@22 {
		compatible = "nxp,pca9555";
